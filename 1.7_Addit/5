import java.io.Console;
import java.util.ArrayList;
import java.util.Date;
import java.util.Scanner;

public class Main {


    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a  = in.nextInt();
        int summ =0;
        while (a > 0)
        {
            summ += a % 10;
            a = a/10;
        }
        System.out.println(summ);

    }

}
