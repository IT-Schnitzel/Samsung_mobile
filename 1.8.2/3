    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a  = in.nextInt();
        int [] A = new int [a];
        for (int i = 0; i < a; i ++)
        {
            A[i] = in.nextInt();
        }
        for (int i = 0 ; i < a; i ++)
        {
            if (A[i] % 2 == 0)
            {
                System.out.print(A[i] + " ");
            }
        }
    }

}
